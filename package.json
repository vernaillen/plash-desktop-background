{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lagon:dev": "NITRO_PRESET=lagon pnpm build && npx -p esbuild -p @lagon/cli lagon dev",
