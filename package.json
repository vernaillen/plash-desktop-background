    "vercel:deploy": "NITRO_PRESET=vercel-edge vercel deploy"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.6",
    "@types/node": "^22.0.0",
    "@unocss/nuxt": "^0.58.0",
    "@unocss/preset-web-fonts": "^0.58.0",
    "nuxt": "^3.8.2",
    "nuxt-time": "^0.1.1"
  },
  "dependencies": {
    "gsap": "^3.12.4"
  }
}
